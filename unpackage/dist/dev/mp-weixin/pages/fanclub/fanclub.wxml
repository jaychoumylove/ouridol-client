<view class="funclub-container" bind:__l="__l"><block wx:for="{{article}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{item.title}}"><view class="article-title">{{item.title}}</view></block><block wx:for="{{item.content}}" wx:for-item="item1" wx:for-index="index1" wx:key="index1"><block wx:if="{{item.content.length>0}}"><text class="article-content" decode>{{item1}}</text></block></block><block wx:if="{{item.image}}"><image class="article-image" src="{{item.image}}" mode="widthFix"></image></block></block></block><view class="article-title">申请资料</view><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" class="form-container" bindsubmit="__e"><view class="input-group"><view class="name">后援会头像</view><view data-event-opts="{{[['tap',[['uploadImg',['avatar']]]]]}}" class="img-input flex-set" bindtap="__e"><block wx:if="{{tmpImg.avatar}}"><image src="{{tmpImg.avatar}}" mode="aspectFill"></image></block><block wx:else><view>+</view></block></view></view><view class="input-group"><view class="name">后援会名称</view><input type="text" name="clubname" placeholder="申请入驻后援会的名称"/></view><view class="input-group"><view class="name">负责人姓名</view><input type="text" name="name"/></view><view class="input-group"><view class="name">负责人职位</view><input type="text" name="post" placeholder="你在后援会的职位是什么"/></view><view class="input-group"><view class="name">负责人微信</view><input type="text" name="wx" placeholder="你的微信帐号是什么"/></view><view class="input-group"><view class="name">资质截图</view><view data-event-opts="{{[['tap',[['uploadImg',['qualify']]]]]}}" class="img-input flex-set" bindtap="__e"><block wx:if="{{tmpImg.qualify}}"><image src="{{tmpImg.qualify}}" mode="aspectFill"></image></block><block wx:else><view>+</view></block></view><view class="tips">你的后援会职位证明截图</view></view><view class="input-group" style="padding-top:40rpx;"><button style="width:180rpx;margin:auto;" form-type="submit"><btn-component type="default" vue-slots="{{['default']}}"><view class="flex-set" style="width:180rpx;height:90rpx;">提交</view></btn-component></button></view></form></view>